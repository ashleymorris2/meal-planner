{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "recipes": {
        "ClusterId": "recipe-cluster",
        "Match": {
          "Path": "/recipes/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "api/recipes/{**catch-all}"
          }
        ]
      },
      "images": {
        "ClusterId": "recipe-cluster",
        "Match": {
          "Path": "/images/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "api/images/{**catch-all}"
          }
        ]
      }
    }
  }
}
